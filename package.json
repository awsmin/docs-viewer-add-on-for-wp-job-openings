{
	"name": "docs-viewer-add-on-for-wp-job-openings",
	"version": "1.0.0",
	"description": "Docs Viewer is an add-on for WP Job Openings plugin. This plugin allows you to view the applicant resume from admin panel.",
	"main": "gulpfile.js",
	"scripts": {
		"start": "gulp",
		"watch": "cross-env NODE_ENV=development gulp watch",
		"build": "cross-env NODE_ENV=production gulp build",
		"archive": "cross-env git archive --format=zip --prefix=docs-viewer-add-on-for-wp-job-openings/ --output=./build/docs-viewer-add-on-for-wp-job-openings.zip --worktree-attributes master",
		"dev-archive": "cross-env git archive --format=zip --prefix=docs-viewer-add-on-for-wp-job-openings/ --output=./build/docs-viewer-add-on-for-wp-job-openings.zip --worktree-attributes dev-branch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/Nithi22/awsm-jobs-docs-viewer-add-on.git"
	},
	"keywords": [
		"jobs plugin",
		"wp job openings",
		"resume viewer",
		"docs viewer",
		"cv viewer",
		"job listing",
		"job openings",
		"job board",
		"careers page",
		"jobs page",
		"jobs"
	],
	"author": "AWSM Innovations",
	"license": "GPL-2.0",
	"homepage": "https://awsm.in",
	"devDependencies": {
		"browser-sync": "^2.24.4",
		"cross-env": "^5.2.0",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^5.0.0",
		"gulp-clean-css": "^3.9.4",
		"gulp-concat": "^2.6.1",
		"gulp-help": "^1.6.1",
		"gulp-jshint": "^2.1.0",
		"gulp-line-ending-corrector": "^1.0.2",
		"gulp-rename": "^1.3.0",
		"gulp-sort": "^2.0.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-strip-debug": "^3.0.0",
		"gulp-uglify": "^3.0.0",
		"gulp-wp-pot": "^2.3.2"
	},
	"browserslist": [
		"last 2 version",
		"> 1%",
		"ie >= 9",
		"ie_mob >= 10",
		"ff >= 30",
		"chrome >= 34",
		"safari >= 7",
		"opera >= 23",
		"ios >= 7",
		"android >= 4",
		"bb >= 10"
	]
}
